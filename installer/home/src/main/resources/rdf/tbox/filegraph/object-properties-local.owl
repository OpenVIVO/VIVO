    <!-- http://vivoweb.org/ontology/core#dateEstablished -->

    <owl:ObjectProperty rdf:about="http://vivoweb.org/ontology/core#dateEstablished">

        <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
        <rdfs:range rdf:resource="http://vivoweb.org/ontology/core#DateTimeValue"/>
        <rdfs:subPropertyOf rdf:resource="http://vivoweb.org/ontology/core#dateTimeValue"/>
    </owl:ObjectProperty>

    <!-- http://vivoweb.org/ontology/core#hasContactInfo -->

    <owl:ObjectProperty rdf:about="http://vivoweb.org/ontology/core#hasContactInfo">

        <rdfs:range rdf:resource="http://purl.obolibrary.org/obo/ARG_2000379"/>
        <rdfs:subPropertyOf rdf:resource="http://purl.obolibrary.org/obo/ARG_2000028"/>
    </owl:ObjectProperty>

    <!-- http://vivoweb.org/ontology/core#contactInfoFor -->

    <owl:ObjectProperty rdf:about="http://vivoweb.org/ontology/core#contactInfoFor">

        <owl:inverseOf rdf:resource="http://vivoweb.org/ontology/core#hasContactInfo"/>
        <rdfs:domain rdf:resource="http://purl.obolibrary.org/obo/ARG_2000379"/>
        <rdfs:subPropertyOf rdf:resource="http://purl.obolibrary.org/obo/ARG_2000029"/>
    </owl:ObjectProperty>

    <!-- http://vivoweb.org/ontology/core#hasAffiliatedOrganization -->

    <owl:ObjectProperty rdf:about="http://vivoweb.org/ontology/core#hasAffiliatedOrganization">

        <owl:inverseOf rdf:resource="http://vivoweb.org/ontology/core#hasAffiliatedOrganization"/>
        <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
        <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
    </owl:ObjectProperty>
